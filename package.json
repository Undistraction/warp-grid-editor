{
  "name": "warp-grid-editor",
  "version": "1.1.0",
  "packageManager": "pnpm@9.6.0+sha256.dae0f7e822c56b20979bb5965e3b73b8bdabb6b8b8ef121da6d857508599ca35",
  "description": "Editor/demo for warp-grid",
  "homepage": "https://warp-grid.undistraction.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/undistraction/warp-grid-editor.git"
  },
  "bugs": {
    "url": "https://github.com/undistraction/warp-grid-editor/issues"
  },
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest",
    "test-coverage": "vitest --coverage",
    "test-ui": "ACTIVE_ENV=test playwright test",
    "test-ui-ui": "ACTIVE_ENV=test playwright test --ui",
    "test-ui-update-screenshots": "ACTIVE_ENV=test playwright test --update-snapshots",
    "lint": "pnpm run lint-prettier && pnpm run lint-eslint",
    "lint-prettier": "prettier . --write",
    "lint-eslint": "eslint  --fix './**/*.{js,jsx,cjs}'",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "link": "pnpm link ../warp-grid && pnpm link ../coons-patch",
    "unlink": "pnpm unlink ../warp-grid && pnpm unlink ../coons-patch"
  },
  "keywords": [
    "coons patch",
    "geometry",
    "math",
    "surface",
    "grid"
  ],
  "author": "Pedr Browne",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.6.0",
    "@playwright/test": "^1.46.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@testing-library/react": "^16.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.19",
    "commitlint": "^19.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-vite": "^2.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwind": "^0.2.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.39",
    "prettier": "3.3.2",
    "prettier-plugin-ejs": "^1.0.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "bezier-easing": "^2.1.0",
    "bezier-js": "^6.1.4",
    "classnames": "^2.5.1",
    "dasherize": "^2.0.0",
    "fast-memoize": "^2.5.2",
    "prop-types": "^15.8.1",
    "ramda": "^0.30.1",
    "ramda-adjunct": "^5.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-syntax-highlighter": "^15.5.0",
    "tailwind-merge": "^2.5.0",
    "use-debounce": "^10.0.1",
    "uuid": "^10.0.0",
    "warp-grid": "^1.6.0",
    "zustand": "^4.5.4"
  }
}
